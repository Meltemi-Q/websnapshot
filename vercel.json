{
    "functions": {
      "api/screenshot.js": {
        "maxDuration": 30
      }
    },
    "routes": [
      {
        "src": "/",
        "dest": "/public/index.html"
      },
      {
        "src": "/api/screenshot",
        "dest": "/api/screenshot.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
}