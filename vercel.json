{
    "functions": {
      "api/screenshot.js": {
        "maxDuration": 60,
        "memory": 1024
      }
    },
    "routes": [
      {
        "src": "/",
        "dest": "/public/index.html"
      },
      {
        "src": "/api/screenshot",
        "dest": "/api/screenshot.js"
      },
      {
        "src": "/api/health",
        "dest": "/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ],
    "env": {
      "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true"
    }
}